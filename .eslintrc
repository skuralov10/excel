{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "object-curly-spacing": "off",
    "max-len": "off"
  },
  "extends": [
    "eslint:recommended", 
    "google"
  ]
}